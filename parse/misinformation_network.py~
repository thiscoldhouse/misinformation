import json
from bs4 import BeautifulSoup

def parse_references(html):
    soup = BeautifulSoup(html, 'html.parser')
    bib = soup.find_all('ul', {'class': 'ltx_biblist'})
    assert(len(bib) == 1)    
    bib = bib[0]
    for citation in bib.find_all('li'):
        reference = citation.find_all(
            'span', {'class': 'ltx_bibblock'}
        )
        assert(len(reference) == 1)
        reference = reference[0].contents
        
        
    
